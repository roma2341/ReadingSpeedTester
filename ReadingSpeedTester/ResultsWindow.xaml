<Window x:Class="ReadingSpeedTester.ResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReadingSpeedTester"
        mc:Ignorable="d"
        Title="ResultsWindow" Height="300" Width="300">
    <Grid>
        <WrapPanel Orientation="Vertical">
            <DockPanel>
                <Button x:Name="btnAnalyzeCharactersPerSecond" Content="Символів за час" PreviewMouseDown="btnAnalyzeCharactersPerSecond_PreviewMouseDown" Width="112"/>
            </DockPanel>
        </WrapPanel>
    </Grid>
</Window>
